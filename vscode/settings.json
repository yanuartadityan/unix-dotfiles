{
  "workbench.colorTheme": "Xcode Default Dark Customized Version",
  "workbench.iconTheme": "material-icon-theme",
  "editor.tabSize": 2,
  // "cmake.buildDirectory": "${userHome}/Workspace/builds/${workspaceRootFolderName}/${variant:customer}/${buildType}",
  "cmake.configureOnOpen": true,
  "editor.minimap.maxColumn": 80,
  "[python]": {
    "editor.formatOnType": true
  },
  "discord.removeRemoteRepository": true,
  "discord.removeLowerDetails": true,
  "files.associations": {
    "*.py": "python"
  },
  "todohighlight.isEnable": true,
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'monospace', monospace",
  "editor.fontLigatures": false,
  "gitlens.autolinks": [
    {
      "prefix": "TC-",
      "url": "https://track.smarteye.se/youtrack/issue/TC-<num>"
    },
    {
      "prefix": "DMF-",
      "url": "https://track.smarteye.se/youtrack/issue/DMF-<num>"
    }
  ],
  "vim.easymotion": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "n"],
      "commands": [":nohl"]
    }
  ],
  "vim.handleKeys": {
    "<C-f>": false,
    "<C-a>": false,
    "<C-w>": false,
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false,
    "<C-j>": false,
    "<C-b>": false,
    "<C-k>": false,
    "<C-S-E>": false,
    "<C-S-D>": false
  },
  "vim.smartRelativeLine": true
}
